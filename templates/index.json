        "image_YkUhCe": {
          "type": "image",
          "settings": {}
        },
        "review_CMNCfP": {
          "type": "review",
          "settings": {
            "stars": 5,
            "title": "Je recommande !",
            "description": "<p>Ma chienne s'appelle Loupiote, elle a 9 ans et souffre de plus en plus de ses articulations. Avec votre formule elle se lève plus facilement le matin et reprend de la vitalité. Elle a ses habitudes tous les matins, elle attend sa bouchée.</p>",
            "footer_text": "<strong>Séverine S.</strong>"
